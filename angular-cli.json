{
  "project": {
    "version": "1.0.0-beta.14",
    "name": "baiding-web"
  },
  "apps": [
    {
      "root": "src",
      "outDir": "dist",
      "publicPath": "/",
      "assets": ["assets"],
      "index": "index.html",
      "main": "main.ts",
      "async": false,
      "test": "test.ts",
      "tsconfig": "tsconfig.json",
      "prefix": "app",
      "mobile": false,
      "styles": [
        "styles.scss",
        "../node_modules/video.js/dist/video-js.min.css"
      ],
      "styleVariables": "css/_variables.scss",
      "scripts": [
        "../node_modules/jquery/dist/jquery.js",
        "../node_modules/lodash/lodash.js",
        "../node_modules/leancloud-push/src/AV.push.js",
        "../node_modules/hammerjs/hammer.js",
        "../node_modules/moment/moment.js",
        "../node_modules/moment-duration-format/lib/moment-duration-format.js",
        "../node_modules/moment/locale/zh-cn.js",
        "../node_modules/fastclick/lib/fastclick.js",
        "../node_modules/video.js/dist/video.min.js",
        "../node_modules/video.js/dist/lang/zh-CN.js",
        "../node_modules/videojs-contrib-hls/dist/videojs-contrib-hls.min.js",
        "../node_modules/weixin-js-sdk/index.original.js"
      ],
      "environments": {
        "source": "environments/environment.ts",
        "dev": "environments/environment.dev.ts",
        "prod": "environments/environment.prod.ts",
        "test-prod": "environments/environment.test-prod.ts"
      }
    }
  ],
  "addons": [],
  "packages": [],
  "e2e": {
    "protractor": {
      "config": "./protractor.conf.js"
    }
  },
  "test": {
    "karma": {
      "config": "./karma.conf.js"
    }
  },
  "defaults": {
    "styleExt": "scss",
    "prefixInterfaces": false,
    "inline": {
      "style": false,
      "template": false
    },
    "spec": {
      "class": false,
      "component": true,
      "directive": true,
      "module": false,
      "pipe": true,
      "service": true
    }
  }
}
